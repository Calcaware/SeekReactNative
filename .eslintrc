{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "react-native/react-native": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:react-native/all"],
  "plugins": [
    "react-native"
  ],
  "rules": {
    "quotes": [2, "double"],
    "comma-dangle": [2, "never"],
    "space-in-parens": [2, "always"],
    "func-names": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-curly-spacing": 0
  }
}